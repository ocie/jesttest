{
    "compilerOptions": {
        "removeComments": true,
        "moduleResolution": "node",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "esModuleInterop": true,
        "sourceMap": true,
        "target": "es2020",
        "module": "commonjs",
        "outDir": "./dist"
    },
    "include": ["./**/*.ts"],
    "exclude": ["node_modules/**/*", ".serverless/**/*", ".webpack/**/*", "_warmup/**/*", ".vscode/**/*", "**/*.spec.*", "e2e"],
}
